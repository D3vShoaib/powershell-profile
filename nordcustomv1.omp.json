{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [

          {
            "background": "#b18bb1",
            "foreground": "#d9cbb5",
            "leading_diamond": "\u256d\u2500\ue0b6",
            "style": "diamond",
            "template":"{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
            "type": "session"
          },
          {
            "background": "#90adc6",
            "foreground": "#292828",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "style":"full"
            },
            
            "style": "powerline",
            "template": " \uf07b </>{{ .Path }} ",
            "type": "path"
          },
          {
            "background": "#9fca9f",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#f2a272{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f4d58d{{ end }}",
              "{{ if gt .Ahead 0 }}#b18bb1{{ end }}",
              "{{ if gt .Behind 0 }}#b18bb1{{ end }}"
            ],
            "foreground": "#292828",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          },
          {
            "background": "#e28f92",
            "foreground": "#292828",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "always_enabled": true
            },
            "style": "powerline",
            "template": " \ueba2 {{ .FormattedMs }} ",
            "type": "executiontime"
          },
          {
            "background": "#f4d58d",
            "background_templates": [
              "{{ if gt .Code 0 }}#e28f92{{ end }}"
            ],
            "foreground": "#292828",
            "properties": {
              "always_enabled": true
            },
            "style": "diamond",
            "template": " {{ if gt .Code 0 }}\uf00d {{ reason .Code }}{{ else }}\uf42e {{ end }}",
            "trailing_diamond": "\ue0b0",
            "type": "status"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "#f2a272",
            "foreground": "#292828",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "style": "powerline",
            "template": "\uf489 {{ .Name }} </>",
            "type": "shell"
          },
          {
            "background": "#9fca9f",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "background_templates": [
              "{{if eq \"Charging\" .State.String}}#9fca9f{{end}}",
              "{{if eq \"Discharging\" .State.String}}#9fca9f{{end}}",
              "{{if eq \"Full\" .State.String}}#9fca9f{{end}}"
            ],
            "foreground": "#292828",
            "style": "powerline",
            "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 \ueb2d ",
            "type": "battery"
          },
          {
            "background": "#90adc6",
            "foreground": "#292828",
            "invert_powerline": true,
            "trailing_diamond": "\ue0b4",
            "style": "diamond",
            "template": " \ue641 {{ .CurrentDate | date .Format }} ",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#d9cbb5",
            "style": "plain",
            "template": "\u2570\u2500",
            "type": "text"
          },
          {
            "foreground": "#b18bb1",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#e28f92{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "<#b18bb1>\u276f</><#9fca9f>\u276f</><#f4d58d>\u276f</> ",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "version": 2
}
